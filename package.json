{
  "name": "dela-iframe-test",
  "version": "1.0.0",
  "description": "A github page to test an embedded application form in an iframe.",
  "author": "David Kurek",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviddomain/dela-iframe-test.git"
  },
  "bugs": {
    "url": "https://github.com/daviddomain/dela-iframe-test/issues"
  },
  "homepage": "https://daviddomain.github.io/dela-iframe-test/",
  "publishConfig": {
